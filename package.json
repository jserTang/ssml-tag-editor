{
  "name": "ssml-tag-editor",
  "version": "1.0.9",
  "description": "SSML tag editor;Generate SSML with rich text editing;Text-to-Speech editor",
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "npx webpack serve --node-env=development --progress --config ./webpack.config.js",
    "build:watch": "npx webpack --watch --node-env=production --progress --config ./webpack.config.js",
    "build": "npx webpack --node-env=production --progress --config ./webpack.config.js && tsc --emitDeclarationOnly && npm link",
    "pub": "npm run build && npm publish"
  },
  "dependencies": {
    "@tiptap/extension-character-count": "2.2.4",
    "@tiptap/extension-color": "2.2.4",
    "@tiptap/extension-placeholder": "2.2.4",
    "@tiptap/extension-text-style": "2.2.4",
    "@tiptap/pm": "2.2.4",
    "@tiptap/react": "2.2.4",
    "@tiptap/starter-kit": "2.2.4",
    "@tiptap/suggestion": "2.2.4"
  },
  "devDependencies": {
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "postcss-loader": "^7.3.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "css-loader": "^6.8.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "react-refresh": "^0.14.0",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jserTang/ssml-tag-editor.git"
  },
  "keywords": [
    "react",
    "react-component",
    "SSML",
    "SSML editor"
  ],
